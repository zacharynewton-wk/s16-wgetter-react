{
  "name": "s16-wgetter-react",
  "description": "Desktop app for section 16 wgetter",
  "license": "MIT",
  "homepage": ".",
  "repository": "zacharynewton-wk/electron-playground",
  "author": {
    "name": "Zach Newton",
    "email": "zach.newton@workiva.com"
  },
  "version": "0.1.0",
  "main": "electron-starter.js",
  "private": true,
  "dependencies": {
    "electron-debug": "1.4.0",
    "eslint": "^4.17.0",
    "global": "^4.3.2",
    "node-sass-chokidar": "^0.0.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.1.0",
    "wget-improved": "^1.5.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "devtron": "1.4.0",
    "electron": "1.7.11",
    "electron-builder": "^19.52.1",
    "js-beautify": "^1.7.5",
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.2",
    "sinon": "^4.1.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron-dev": "ELECTRON_START_URL=http://localhost:3000 electron .",
    "electron-start": "electron .",
    "electron-build": "npm-run-all electron-build-mac electron-build-win electron-build-linux",
    "electron-build-mac": "electron-builder -m",
    "electron-build-win": "electron-builder -w",
    "electron-build-linux": "electron-builder -l",
    "electron-dist": "electron-builder -mwl",
    "format": "js-beautify --type js  -f ./src/*"
  }
}
